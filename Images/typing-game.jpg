<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#161616"/>
  <rect x="100" y="150" width="600" height="300" rx="15" fill="#346751" opacity="0.2"/>
  
  <!-- Keyboard keys -->
  <g transform="translate(200, 250)">
    <!-- Row 1 -->
    <rect x="0" y="0" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="50" y="0" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="100" y="0" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="150" y="0" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="200" y="0" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="250" y="0" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="300" y="0" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="350" y="0" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    
    <!-- Row 2 -->
    <rect x="25" y="50" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="75" y="50" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="125" y="50" width="40" height="40" rx="5" fill="#C84B31" opacity="0.9"/>
    <rect x="175" y="50" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="225" y="50" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="275" y="50" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    <rect x="325" y="50" width="40" height="40" rx="5" fill="#ECDBBA" opacity="0.8"/>
    
    <!-- Row 3 (Space bar) -->
    <rect x="75" y="100" width="250" height="30" rx="5" fill="#ECDBBA" opacity="0.8"/>
  </g>
  
  <!-- Text elements -->
  <text x="400" y="150" font-family="Arial, sans-serif" font-size="40" font-weight="bold" fill="#ECDBBA" text-anchor="middle">Speed Typer</text>
  <text x="400" y="190" font-family="Arial, sans-serif" font-size="20" fill="#ECDBBA" opacity="0.7" text-anchor="middle">Test your typing skills</text>
  
  <!-- Stats display -->
  <g transform="translate(250, 400)">
    <rect x="0" y="0" width="100" height="80" rx="10" fill="#346751" opacity="0.3"/>
    <text x="50" y="30" font-family="Arial, sans-serif" font-size="16" fill="#ECDBBA" text-anchor="middle">WPM</text>
    <text x="50" y="60" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#ECDBBA" text-anchor="middle">75</text>
    
    <rect x="120" y="0" width="100" height="80" rx="10" fill="#346751" opacity="0.3"/>
    <text x="170" y="30" font-family="Arial, sans-serif" font-size="16" fill="#ECDBBA" text-anchor="middle">Accuracy</text>
    <text x="170" y="60" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#ECDBBA" text-anchor="middle">98%</text>
    
    <rect x="240" y="0" width="100" height="80" rx="10" fill="#346751" opacity="0.3"/>
    <text x="290" y="30" font-family="Arial, sans-serif" font-size="16" fill="#ECDBBA" text-anchor="middle">Time</text>
    <text x="290" y="60" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#ECDBBA" text-anchor="middle">30s</text>
  </g>
</svg>